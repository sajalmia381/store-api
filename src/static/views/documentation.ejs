                </div>
                <!-- User Link -->
                <div class="link__group mt-5">
                    <h3 class="link__group__heading">User</h3>
                    <nav class="link__nav mt-3">
                        <a href="#id_user--1">Get all users</a>
                        <a href="#id_user--2">Get a single user</a>
                        <a href="#id_user--3">Add user</a>
                        <a href="#id_user--4">Update user</a>
                        <a href="#id_user--5">Delete user</a>
                    </nav>
                </div>
                <!-- Auth Link -->
                <div class="link__group mt-5">
                    <h3 class="link__group__heading">Auth</h3>
                    <nav class="link__nav mt-3">
                        <a href="#id_auth--1">Login</a>
                        <a href="#id_auth--2">Register</a>
                        <a href="#id_auth--3">Refresh Token</a>
                    </nav>
                </div>
                
                <!-- User Cart -->
                <div class="link__group mt-5">
                    <h3 class="link__group__heading">Cart</h3>
                    <p>Coming soon</p>
                    <!-- <nav class="link__nav mt-3">
                        <a href="#id_cart--1">Get all Carts</a>
